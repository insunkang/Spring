package app3;
/*
	스프링을 이용해서 프로그램을 실행하기
*/
public class MyBeanTest {

	public static void main(String[] args) {
		MyBeanStyleA obj = new MyBeanStyleA();
		run(obj);
		show(obj);

	}
	public static void run(MyBeanStyle obj) {
		System.out.println("******************");
		obj.hello("현빈");
		obj.hello("현빈");
		System.out.println("******************");
	}
	public static void show(MyBeanStyle obj) {
		System.out.println("==================");
		obj.hello("현빈");
		obj.hello("현빈");
		System.out.println("==================");
	}
}
