package app1;
/*
	결합도가 극도로 높은프로그램
	=> 클래스간의 결합도가 강하고 의존성이 높다.

*/
public class MyBeanTest {

	public static void main(String[] args) {
		MyBeanStyleA obj = new MyBeanStyleA();
		run(obj);
		show(obj);

	}
	public static void run(MyBeanStyleA obj) {
		System.out.println("******************");
		obj.testHello("현빈");
		obj.testHello("현빈");
		System.out.println("******************");
	}
	public static void show(MyBeanStyleA obj) {
		System.out.println("==================");
		obj.testHello("현빈");
		obj.testHello("현빈");
		System.out.println("==================");
	}
}
