package app1;

public class MyBeanTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}
	public static void run(MyBeanStyleA obj) {
		System.out.println("******************");
		obj.testHello("泅后");
		obj.testHello("泅后");
		System.out.println("******************");
	}
	public static void show(MyBeanStyleB obj) {
		System.out.println("==================");
		obj.testHello("泅后");
		obj.testHello("泅后");
		System.out.println("==================");
	}
}
