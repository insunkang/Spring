public class MyBeanTest {

	public static void main(String[] args) {
		MyBeanStyleA obj = new MyBeanStyleA();
		run(obj);
		show(obj);

	}
	public static void run(MyBeanStyle obj) {
		System.out.println("******************");
		obj.hello("Çöºó");
		obj.hello("Çöºó");
		System.out.println("******************");
	}
	public static void show(MyBeanStyle obj) {
		System.out.println("==================");
		obj.hello("Çöºó");
		obj.hello("Çöºó");
		System.out.println("==================");
	}
}
